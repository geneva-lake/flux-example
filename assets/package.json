{
  "name": "out-of-line",
  "version": "0.0.1",
  "description": "Outline Editor that uses Reactjs and the Flux architecture",
  "main": "app.js",
  "dependencies": {
    "classnames": "^2.1.3",
    "flux": "2.0.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "react": "15.4.0"
  },
  "devDependencies": {
    "browserify": "~4.2.2",
    "envify": "~2.0.1",
    "jest-cli": "~0.1.17",
    "reactify": "~0.14.0",
    "statics": "~0.1.0",
    "uglify-js": "~2.4.15",
    "watchify": "~0.10.2"
  },
  "scripts": {
    "start": "watchify -o bundle.js -v -d app.js",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > bundle.min.js",
    "collect-static": "collect-static . ./static"
  },
  "author": "Nicola Paolucci",
  "license": "Apache 2",
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}
